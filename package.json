{
    "name": "agendash",
    "version": "0.0.0",
    "description": "UI package for agendash scheduler",
    "main": "lib/index.js",
    "directories": {
        "lib": "lib"
    },
    "scripts": {
        "start": "node ./demo/index.js",
        "start:pm2": "./node_modules/.bin/pm2 ecosystem.config.js",
        "install": "npm i && cd ui && npm i",
        "install:prod": "npm i --production && cd ui && npm i --production",
        "build": "./ui/node_modules/.bin/ng build",
        "build:watch": "./ui/node_modules/.bin/ng build --watch",
        "build:prod": "./ui/node_modules/.bin/ng build --configuration=prod",
        "setup":"npm run install && npm run start:pm2 && npm run build",
        "test": "npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/manojc/agendash.git"
    },
    "keywords": [
        "agenda"
    ],
    "author": "chalodem@gmail.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/manojc/agendash/issues"
    },
    "homepage": "https://github.com/manojc/agendash#readme",
    "dependencies": {
        "fs": "0.0.1-security",
        "http": "0.0.0",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "pm2": "^3.2.4"
    }
}
